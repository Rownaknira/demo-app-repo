<%	if	logged_in?	%>
<h1>Listing Users</h1>
<table>
<thead>
<tr>
<th>User</th>
<th></th>
</tr>
</thead>
<% @users.each do |user| %>
<% if !current_user?(user)%>
<tr>
<td><%= user.name %></td>
<td><%= link_to 'Show', :action => 'show', :id => user.id  %></td>
</tr>
<% end %>
<% end %>
</table>
<br>
<% else%>
    <li><%=	link_to	"Login",	login_path	%></li>
<% end%> 